<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxWidth="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" stylesheets="/css/application.css">
   <children>
      <TextArea fx:id="textArea" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true">
          <VBox.margin>
              <Insets left="10.0" right="10.0" top="10" />
          </VBox.margin>
      </TextArea>
      <GridPane prefWidth="580.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="134.4" prefWidth="151.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="443.6" minWidth="10.0" prefWidth="120.19999999999999" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="443.6" minWidth="10.0" prefWidth="115.59999999999997" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="443.6" minWidth="10.0" prefWidth="193.60000000000002" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1">
               <children>
                   <Button fx:id="frontShiftLeft" mnemonicParsing="false" text="&lt;" />
                   <Button fx:id="frontShiftRight" mnemonicParsing="false" text="&gt;" />
<!--                   <RadioButton fx:id="beginningButton" mnemonicParsing="false" selected="true" text="%at_the_beginning">-->
<!--                     <toggleGroup>-->
<!--                        <ToggleGroup fx:id="position" />-->
<!--                     </toggleGroup></RadioButton>-->
<!--                   <RadioButton fx:id="endButton" mnemonicParsing="false" text="%at_the_end" toggleGroup="$position" />-->
               </children>
            </HBox>
            <Label alignment="CENTER" text="%modify_the_beginning" GridPane.columnIndex="0" GridPane.rowIndex="0">
               <GridPane.margin>
                  <Insets left="10.0" right="10.0" />
               </GridPane.margin>
            </Label>
             <Label alignment="CENTER" text="%modify_the_end" GridPane.columnIndex="2">
                 <GridPane.margin>
                     <Insets left="10.0" right="10.0" />
                 </GridPane.margin>
             </Label>
            <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="3">
               <children>
                  <Button fx:id="endShiftLeft" mnemonicParsing="false" text="&lt;" />
                  <Button fx:id="endShiftRight" mnemonicParsing="false" text="&gt;" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </GridPane>
      <Separator prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Separator>
      <HBox alignment="CENTER_RIGHT" spacing="15.0">
         <children>
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" text="%cancel" />
            <Button fx:id="confirmButton" defaultButton="true" mnemonicParsing="false" text="%confirm" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
